<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Homuai.App.Views.Templates.Button.ConfirmDangerActionButton">
    <ContentView.Content>
        <Grid ColumnSpacing="30" ColumnDefinitions="Auto, *" RowDefinitions="40">
            <Button x:Name="Button" Clicked="Button_OnTapped" Grid.Column="0" HeightRequest="40" Style="{x:StaticResource ButtonDangerActionDefault}" Padding="15,0,15,0"></Button>
        </Grid>
    </ContentView.Content>
</ContentView>